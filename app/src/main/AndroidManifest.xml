<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.workout">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DisplayWorkout2" />
        <activity android:name=".DisplayWorkoutCategories" />
        <activity android:name=".PPLProgram.DisplayPPLProgram" />
        <activity android:name=".DisplayAbout" />
        <activity android:name=".PPLProgram.DisplayAboutPPL" />
        <activity android:name=".PPLProgram.DisplayPPLInter" />
        <activity android:name=".PPLProgram.DisplayLegsA" />
        <activity android:name=".WorkoutGoal" />
        <activity android:name=".StrongLift" />
        <activity android:name=".PPLProgram.DisplayPushA" />
        <activity android:name=".PPLProgram.DisplayPullA" />
        <activity android:name=".CardioProgramDir.Cardio" />
        <activity android:name=".CardioProgramDir.CardioProgram" />
        <activity android:name=".UserInfo" />
        <activity android:name=".DisplayWeightGraph" />
        <activity android:name=".DisplaySettings" />
        <activity
            android:name=".DisplayCheckpage"
            android:excludeFromRecents="true"
            android:launchMode="singleTask"
            android:taskAffinity="" />
        <activity android:name=".MaxRepCalc"></activity>
    </application>

</manifest>